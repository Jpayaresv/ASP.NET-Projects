@model List<Alumno>

@{
    ViewData["Title"] = "Informaci√≥n Alumno";
    Layout = "Simple";
}


        @foreach (var alumno in Model)
        {
            <partial name="ObjetoEscuelaSimple" model="alumno"/>
        }
        
<h1>@ViewData["Title"]</h1>


<table class="table table-dark table-hover">
    <thead>
      <tr>  
        <tr scope="col">Nombre</tr>
        <tr scope="col">Id</tr>
      </tr>
    </thead>

    <tbody>
        @foreach (var alumno in Model)
        {
            <tr>
                <td>@alumno.Nombre</td>
                <td>@alumno.UniqueId</td>

            </tr>       
        }

    </tbody>

</table>


<p>@ViewBag.Fecha</p>